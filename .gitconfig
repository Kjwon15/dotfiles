[user]
	name = kjwon15
	email = kjwonmail@gmail.com
[core]
	pager = less -FRX
	excludesfile = ~/.gitignore
[color]
	ui = true
[alias]
	co = checkout
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --author-date-order
	lga = log --all --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --author-date-order
	df = diff -b --color-words
	fix = commit --amend -C HEAD
	st = status -sb
[push]
	default = matching
	followTags = true
[diff]
	tool = vimdiff
	compactionHeuristic = true
[merge]
	tool = vimdiff
	conflictstyle = diff3
