[core]
s0_active_plugins = core;composite;opengl;vpswitch;resize;compiztoolbox;place;imgpng;snap;put;session;copytex;commands;regex;grid;winrules;unitymtgrabhandles;move;wall;mousepoll;animation;expo;ezoom;workarounds;fade;scale;unityshell;
s0_autoraise = false
s0_autoraise_delay = 500
s0_close_window_key = <Alt>q
s0_window_menu_key = Disabled
s0_hsize = 3
s0_vsize = 2

[rotate]
s0_rotate_left_key = <Control><Alt>h
s0_rotate_right_key = <Control><Alt>l
s0_rotate_left_window_key = <Shift><Control><Alt>h
s0_rotate_right_window_key = <Shift><Control><Alt>l

[staticswitcher]
s0_next_key = Disabled
s0_prev_key = Disabled

[winrules]
s0_skiptaskbar_match = class=conky
s0_skippager_match = class=conky
s0_below_match = class=conky
s0_sticky_match = class=conky
s0_no_move_match = class=conky
s0_no_resize_match = class=conky
s0_no_focus_match = class=conky

[unityshell]
s0_show_hud = F12
s0_override_decoration_theme = true
s0_inactive_shadow_color = #0000001e
s0_launcher_hide_mode = 1
s0_num_launchers = 1

[wall]
s0_left_key = <Control><Alt>h
s0_right_key = <Control><Alt>l
s0_up_key = <Control><Alt>k
s0_down_key = <Control><Alt>j
s0_left_window_key = <Shift><Control><Alt>h
s0_right_window_key = <Shift><Control><Alt>l
s0_up_window_key = <Shift><Control><Alt>k
s0_down_window_key = <Shift><Control><Alt>j

[gnomecompat]
s0_main_menu_key = <Super>s
s0_command_window_screenshot = gnome-screenshot -w
s0_command_terminal = x-terminal-emulator
s0_run_command_terminal_key = <Alt>k

[place]
s0_multioutput_mode = 1
s0_viewport_matches = class=Firefox;class=Google-chrome;
s0_viewport_x_values = 1;1;
s0_viewport_y_values = 1;2;

[put]
s0_put_next_output_key = <Alt><Super>Right
s0_put_previous_output_key = <Alt><Super>Left

[decor]
s0_shadow_match = (any) & !(class=conky)
